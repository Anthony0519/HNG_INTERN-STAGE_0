
{
    "name": "public-information",
    "headers": [
      {
        "source": "(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/server.js"
      }
    ]
  }
